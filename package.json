{
  "name": "e-commerce-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.4",
    "npm": "6.14.6"
  },
  "description": "",
  "main": "server.ts",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon -r dotenv/config server.ts",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node dist/server.js",
    "heroku-postbuild": "tsc && cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "express": "^4.16.4",
    "stripe": "^8.183.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.9",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeniorIgor/Ecommerce-Clothing.git"
  },
